<!DOCTYPE html>
<html>
<head>
  <title>Wallmaze</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render "layouts/app" %>
<% if !flash[:notice].nil? %>
<div class="alert alert-success success" >
  <%= flash[:notice] %>
</div>
<% end %>
<%= yield %>

</body>
</html>
