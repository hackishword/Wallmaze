
.posts-wrapper.row
  -@posts.each do |post|
    .post
      .post-head
        .name
          =post.user.try(:user_name) || "Anonymous"
      .image.center-block
        =link_to (image_tag post.image.url(:medium), class:'img-responsive'), post_path(post)
        %p.caption
          =post.caption
